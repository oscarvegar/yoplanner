					</td>
				</tr>
				<!-- Comp -->
				<!-- Componente -->
				<tr>

				</tr>
				<!-- Comp -->
				<!-- Componente -->
				<tr>
					<td>
						<div style="width: 100%;">
			        <strong>Asistentes</strong><br>
			        <div style="margin: 1em;border-radius: 5px;border: thin solid rgb(181, 181, 181);padding: 0.5em;"><%= rfp.asistenciaEsperada %></div>
			      </div>
					</td>
				</tr>
				<!-- Comp -->
				<!-- Componente -->
				<tr>
					<td>
						<div style="width: 100%;">
			        <strong>Hab. Sencillas</strong><br>
			        <div style="margin: 1em;border-radius: 5px;border: thin solid rgb(181, 181, 181);padding: 0.5em;">
                <% for (var i = 0; i < rfp.configuracionHabitaciones.length; i++) {
                    if (rfp.configuracionHabitaciones[i].tipoHabitacion == 1) { %>
                      <p><%= moment(rfp.configuracionHabitaciones[i].fecha).format('DD/MM') %> | <%= rfp.configuracionHabitaciones[i].habitaciones %></p>
                  <% }
                  } %>
              </div>
			      </div>
					</td>
				</tr>
				<!-- Comp -->
        <!-- Componente -->
				<tr>
					<td>
						<div style="width: 100%;">
			        <strong>Hab. Dobles</strong><br>
			        <div style="margin: 1em;border-radius: 5px;border: thin solid rgb(181, 181, 181);padding: 0.5em;">
                <% for (var i = 0; i < rfp.configuracionHabitaciones.length; i++) {
                    if (rfp.configuracionHabitaciones[i].tipoHabitacion == 2) { %>
                      <p><%= moment(rfp.configuracionHabitaciones[i].fecha).format('DD/MM') %> | <%= rfp.configuracionHabitaciones[i].habitaciones %></p>
                  <% }
                  } %>
              </div>
			      </div>
					</td>
				</tr>
				<!-- Comp -->
				<!-- Componente -->
				<tr>
					<td>
						<div style="width: 100%;">
			        <strong>Comentarios</strong><br>
			        <div style="margin: 1em;border-radius: 5px;border: thin solid rgb(181, 181, 181);padding: 0.5em;"><%= rfp.comentarios %></div>
			      </div>
					</td>
				</tr>
				<!-- Comp -->
				<!-- Dots -->
				<tr>
					<td>
						<div style="background-color: #D3E203;display: inline-block;width: 60%;text-align: center;color: white;font-size: 2em;padding: 0.5em;margin:1em;border-radius:5px;">
			        <strong>EVENTOS</strong>
			      </div>
					</td>
				</tr>
        <!-- FOR EVENTOS -->
        <% for (var i = 0; i < rfp.salones.length; i++) { %>
          <!-- EVENTO -->
