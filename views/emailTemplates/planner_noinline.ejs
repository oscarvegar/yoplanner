									if (rfp.salones[i].tipoEvento == 5) { %>
										<span>Sesión</span>
									<% }
									if (rfp.salones[i].tipoEvento == 6) { %>
										<span>Breakout</span>
									<% }
									if (rfp.salones[i].tipoEvento == 7) { %>
										<span>Oficina</span>
									<% }
									if (rfp.salones[i].tipoEvento == 8) { %>
										<span>Bodega</span>
									<% }
