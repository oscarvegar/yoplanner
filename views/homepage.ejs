    <div class="tp-banner">
      <!-- SLIDES CONTENT-->
      <ul>
        <!-- SLIDE  -->
        <li data-transition="slidevertical" data-slotamount="1" data-masterspeed="1000" data-saveperformance="off" data-title="Slide">
          <!-- MAIN IMAGE -->
          <img src="img/slide/00.jpg" alt="fullslide1" data-bgposition="center center" data-kenburns="on" data-duration="4000" data-ease="Linear.easeNone" data-bgfit="130" data-bgfitend="100" data-bgpositionend="right center">
          <!-- LAYERS -->
        </li>
        <li data-transition="slidevertical" data-slotamount="1" data-masterspeed="1000" data-saveperformance="off" data-title="Slide">
          <!-- MAIN IMAGE -->
          <img src="img/slide/reception.jpg" alt="fullslide1" data-bgposition="center center" data-kenburns="on" data-duration="4000" data-ease="Linear.easeNone" data-bgfit="130" data-bgfitend="100" data-bgpositionend="right center">
          <!-- LAYERS -->
        </li>
        <!-- SLIDE  -->
        <li data-transition="slidehorizontal" data-slotamount="1" data-masterspeed="1000" data-saveperformance="off" data-title="Slide">
          <!-- MAIN IMAGE -->
          <img src="img/slide/001.jpg" alt="fullslide1" data-bgposition="top center" data-kenburns="on" data-duration="4000" data-ease="Linear.easeNone" data-bgfit="130" data-bgfitend="100" data-bgpositionend="right center">
          <!-- LAYERS -->
        </li>
        <li data-transition="slidevertical" data-slotamount="1" data-masterspeed="1000" data-saveperformance="off" data-title="Slide">
          <!-- MAIN IMAGE -->
          <img src="img/slide/slideotra2.jpg" alt="fullslide1" data-bgposition="center center" data-kenburns="on" data-duration="4000" data-ease="Linear.easeNone" data-bgfit="130" data-bgfitend="100" data-bgpositionend="right center">
          <!-- LAYERS -->
        </li>
      </ul>
      <!-- END SLIDES  -->
      <div class="tp-bannertimer"></div>
    </div>
    <!-- SLIDE CONTENT-->

    <!-- FILTER HEADER - TITLE HEADER-->
    <div class="filter-title" style="padding-top: 20px;">
      <!-- TITLE HEADER-->
      <div class="title-header">
        <h1 style="font-size: 3em;">Encuentra hoteles para grupos, eventos y comenta</h1>
        <p class="lead" style="font-weight: 500;text-shadow: 2px 3px 3px rgba(0,0,0,1);">Yo Planner es la plataforma exclusiva para los profesionales en la industria de grupos y eventos, comparte y recomienda tus experiencias a otros expertos.</p>

      </div>
      <div class="visible-xs hidden-lg hidden-xl text-center">
        <h1 style="font-size: 3em; color: white; text-shadow: 2px 3px 3px rgba(0,0,0,1);">Encuentra hoteles para grupos, eventos y comenta</h1>
        <p class="lead" style="color: white;font-weight: 500;text-shadow: 2px 3px 3px rgba(0,0,0,1);">Yo Planner es la plataforma exclusiva para los profesionales en la industria de grupos y eventos, comparte y recomienda tus experiencias a otros expertos.  </p>
      </div>
      <!-- END TITLE HEADER-->

      <!-- FILTER HEADER-->
      <div class="filter-header col-sm-10 col-md-6 col-centered hidden-xs hidden-sm" ng-controller="AutocompleteController" style="overflow:visible;">
        <form action="#">
          <autocomplete ng-model="searchString" data="cities" on-type="findCities" on-select="searchCity" attr-placeholder="¿Qué destino quieres buscar?"></autocomplete>
          <img src="img/lblue.gif" style="width:30px;height:30px;" ng-show="showLoader || false" class="ng-hide">
        </form>
      </div>
    </div>
    <!-- END FILTERHEADER - TITLE HEADER -->
  </section>
  <!-- End Slide And Filter Section-->

  <!--Content Central -->
  <section class="content-central" ng-controller="HomePageController" style="margin: 3rem; margin-top: -10rem;">
    <!-- Shadow Semiboxed Layout -->
    <div class="semiboxshadow text-center">
      <img src="img/img-theme/shp.png" class="img-responsive" alt="">
    </div>
    <!-- End Shadow Semiboxed Layout -->
    <!-- End content - Slide Features-->
    <div class="content_info content_resalt">
      <!-- content-->
      <div class="container" style="margin-top: 3em;">
        <div class="row">
          <div class="col-xs-12 filter-header visible-xs hidden-lg" ng-controller="AutocompleteController" style="overflow:visible; margin-bottom: 1em;">
            <autocomplete ng-model="searchString" data="cities" on-type="findCities" on-select="searchCity" attr-placeholder="Encuentra Destinos y Hoteles"></autocomplete>
            <img src="img/lblue.gif" style="width:30px;height:30px;" ng-show="showLoader || false" class="ng-hide">
          </div>
          <!-- Info Resalt - Info Services -->
          <div class="col-md-5 col-xs-12">
            <div class="services-lines-info">
              <!-- h2>We Provide You An Ultimate Travel Experience</h2 -->
              <h1>Únete a la comunidad de profesionales en la industria de grupos y eventos. <button type="button" class="btn btn-primary" style="display: inline-block; margin-top: 5px; margin-left: 1rem;" onclick="window.location = '/main/registro'">REGISTRO</button></h1>

              <p class="lead">
                <span class="line"></span>
              </p>

              <p><strong>La única plataforma en línea y en español donde tú como experto tienes la habilidad de calificar y comentar miles de hoteles, destinos y proveedores, así como de accesar a contenido exclusivo y convertirte en líder de opinión en nuestra industria.</p>
              <br>
            </div>
          </div>

          <div class="col-md-7 col-xs-12 text-center" ng-init="getLastFive()">
            <h3>Últimos comentarios</h3>
            <div class="text-center" style="margin-bottom: 1rem;">
              <ul id="testimonials">
                <li ng-repeat="comentario in lastFiveComments" style="background: url('{{comentario.hotel.fotoPrincipal}}') no-repeat center center; background-size: 100%; padding: 1rem; border-radius: 20px;">
                  <p><strong><a href="/{{comentario.hotel.urlslug}}" style="color: white; text-shadow: 2px 3px 3px rgba(0,0,0,1);">{{comentario.hotel.name}}</a></strong></p>
                  <p style="text-shadow: 2px 3px 3px rgba(0,0,0,1); color: white;"><i class="fa fa-quote-left" style="margin-right: 1rem;"></i>{{comentario.text | limitTo:200}}<i class="fa fa-quote-right" style="margin-left: 1rem;"></i></p>

                  <div class="image-testimonials">
                    <img ng-src="{{comentario.user.avatar}}" alt="">
                  </div>
                  <h4 style="color: white; text-shadow: 2px 3px 3px rgba(0,0,0,1);">{{comentario.user.name || comentario.user.fullName || comentario.user.username}}</h4>
                  <h5 style="color: white; text-shadow: 2px 3px 3px rgba(0,0,0,1);">{{comentario.user.empresa || 'Empresa no asignada.'}}</h5>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </div>

    </div>
    <!-- End content - Slide Features-->

    <div class="content_info">
      <!-- Parallax Background -->
      <div class="bg_parallax image_02_parallax"></div>
      <!-- Parallax Background -->

      <div class="opacy_bg_02">
        <!-- Testimonial Propertie-->
        <div class="container">
          <div class="row">
            <div class="col-md-6 text-center" ng-init="loadRankingUsers()" style="padding: 1rem; background-color: rgb(43, 43, 43)">
              <table class="table table-default table-rankings" style="border: 1px solid rgb(82, 82, 82)">
                <thead>
                  <tr>
                    <th>#</th>
                    <th>Foto</th>
                    <th>Nombre</th>
                    <th>Puntos</th>
                  </tr>
                </thead>
                <tbody>
                  <tr ng-repeat="user in rankingUsers | limitTo:5 | orderBy:user.puntos:true">
                    <td>{{$index + 1}}</td>
                    <td><img ng-src="{{user.avatar}}" alt="Avatar {{$index + 1}}" style="width: 64px; height: 64px; border-radius: 50%;"></td>
                    <td>{{user.fullName}}</td>
                    <td class="puntos">{{user.puntos}}</td>
                  </tr>
                </tbody>
              </table>
            </div>

            <div class="col-md-6">
              <div class="testimonial-info">
                <h2>Ranking de Usuarios</h2>
                <P>Conoce los mejores usuarios en nuestra plataforma.</P>
              </div>
            </div>
          </div>
        </div>
        <!-- End Testimonial Propertie-->
      </div>
    </div>
    <!-- End content info - testimonials-->

